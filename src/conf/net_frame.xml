<?xml version="1.0" encoding="UTF-8"?>
<root>
    <bind>
		<listen>
		    <protocol>admin</protocol>
		    <host>0.0.0.0</host>
		    <port>50205</port>
			 <maptype>1</maptype>
		    <transmission>TCP</transmission>
		    <distribution>T1,T2,T3</distribution>
		    <algorithm>1:1:1</algorithm>
		</listen>
		<listen>
		    <protocol>careland.position</protocol>
		    <host>0.0.0.0</host>
		    <port>57785</port>
		    <transmission>TCP</transmission>
		    <distribution>T1,T2,T3</distribution>
		    <algorithm>1:1:1</algorithm>
			<allowedclients>	
				<client>169.254.10.12</client>
				<client>0.0.0.0</client>
			</allowedclients>
		</listen>
		<listen>
		    <protocol>careland.position</protocol>
		    <host>0.0.0.0</host>
		    <unixdomainpath>/data/svc/kposition/example/unix.domain</unixdomainpath>
		    <transmission>UNIXSOCKET</transmission>
		    <distribution>T1,T2,T3</distribution>
		    <algorithm>1:1:1</algorithm>
		</listen>
		<listen>
		    <protocol>careland.position</protocol>
		    <host>0.0.0.0</host>
		    <port>57786</port>
		    <transmission>UDP</transmission>
		    <distribution>T1</distribution>
		    <algorithm>0</algorithm>
		</listen>
		<listen>
		    <protocol>careland.position</protocol>
		    <host>0.0.0.0</host>
		    <port>57787</port>
		    <transmission>UDP</transmission>
		    <distribution>T2</distribution>
		    <algorithm>0</algorithm>
		</listen>
		<listen>
		    <protocol>careland.position</protocol>
		    <host>0.0.0.0</host>
		    <port>57788</port>
		    <transmission>UDP</transmission>
		    <distribution>T3</distribution>
		    <algorithm>0</algorithm>
		</listen>
    </bind>
     <io_thread>
        <thread>
            <name>T1</name>
            <distribution>H1,H2,H3</distribution>
            <algorithm>1:1:1</algorithm>
        </thread>
        <thread>
            <name>T2</name>
            <distribution>H1,H2,H3</distribution>
            <algorithm>1:1:1</algorithm>
        </thread>
        <thread>
            <name>T3</name>
            <distribution>H1,H2,H3</distribution>
            <algorithm>1:1:1</algorithm>
        </thread>
    </io_thread>
    <handle_thread>
        <handle>
            <name>H1</name>
            <distribution>T1,T2,T3</distribution>
            <algorithm>1:1:1</algorithm>
        </handle> 
        <handle>
            <name>H2</name>
            <distribution>T1,T2,T3</distribution>
            <algorithm>1:1:1</algorithm>
        </handle> 
        <handle>
            <name>H3</name>
            <distribution>T1,T2,T3</distribution>
            <algorithm>1:1:1</algorithm>
        </handle> 
    </handle_thread>
	<auxiliary_thread>
		<auxiliary>
			<name>A1</name>
		</auxiliary>
		<auxiliary>
			<name>A2</name>
		</auxiliary>
		<auxiliary>
			<name>A3</name>
		</auxiliary>
	</auxiliary_thread>
	<clearsocket>
			<valuesec>180</valuesec>
			<valuensec>0</valuensec>
			<intervalsec>180</intervalsec>
			<intervalnsec>0</intervalnsec>
	</clearsocket>
	<heartbeat>
			<valuesec>120</valuesec>
			<valuensec>0</valuensec>
			<intervalsec>120</intervalsec>
			<intervalnsec>0</intervalnsec>
	</heartbeat>
	<monitor>
			<valuesec>3</valuesec>
			<valuensec>0</valuensec>
			<intervalsec>3</intervalsec>
			<intervalnsec>0</intervalnsec>
	</monitor>
	<handleiomsgsize>10000</handleiomsgsize>
	<iohandlemsgsize>10000</iohandlemsgsize>
</root>